    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "fast-xml-parser": "^4.2.7",
    "feed": "^4.2.2",
    "graphql": "^16.7.1",
    "graphql-request": "^6.0.0",
    "lunr": "^2.3.9",
    "next": "14.1.3",
    "next-query-params": "^4.2.3",
    "nextjs-progressbar": "^0.0.16",
    "node-html-parser": "^6.1.12",
    "odoo-await": "^3.4.1",
    "pino": "^9.7.0",
