    "@types/react-imgix": "^9.5.0",
    "@types/sharp": "^0.31.0",
    "@types/uuid": "^9.0.7",
    "dotenv-cli": "^7.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
